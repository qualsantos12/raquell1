        let timerInterval;
        let currentTime = 90;

        function showWorkout(day) {
            const workout = workouts[day];
            document.getElementById('dashboard').classList.add('hidden');
            document.getElementById('workout-detail').classList.remove('hidden');
            
            document.getElementById('workout-title').textContent = workout.title;
            document.getElementById('workout-subtitle').textContent = workout.subtitle;
            
            const container = document.getElementById('exercises-container');
            container.innerHTML = '';
            
            workout.exercises.forEach((exercise, index) => {
                const exerciseCard = createExerciseCard(exercise, index + 1);
                container.appendChild(exerciseCard);
            });
        }

        function createExerciseCard(exercise, number) {
            const card = document.createElement('div');
            card.className = 'exercise-card rounded-xl p-6 shadow-lg';
            
            card.innerHTML = `
                <div class="flex items-start space-x-4">
                    <div class="flex-shrink-0">
                        <div class="w-12 h-12 bg-gradient-to-r from-purple-500 to-blue-500 rounded-full flex items-center justify-center text-white font-bold text-lg">
                            ${number}
                        </div>
                    </div>
                    <div class="flex-grow">
                        <div class="flex flex-col lg:flex-row lg:items-start lg:space-x-6">
                            <div class="flex-grow mb-4 lg:mb-0">
                                <h3 class="text-xl font-bold text-gray-800 mb-2">${exercise.name}</h3>
                                <div class="bg-gradient-to-r from-purple-100 to-blue-100 rounded-lg p-3 mb-4">
                                    <div class="flex items-center space-x-4">
                                        <div class="flex items-center space-x-2">
                                            <i class="fas fa-dumbbell text-purple-600"></i>
                                            <span class="font-semibold text-purple-800">${exercise.sets}</span>
                                        </div>
                                        <div class="flex items-center space-x-2">
                                            <i class="fas fa-clock text-blue-600"></i>
                                            <span class="text-blue-800">60-90s descanso</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="space-y-3">
                                    <div>
                                        <h4 class="font-semibold text-gray-700 mb-1">
                                            <i class="fas fa-play-circle text-green-600 mr-2"></i>
                                            Como executar:
                                        </h4>
                                        <p class="text-gray-600 leading-relaxed">${exercise.instructions}</p>
                                    </div>
                                    <div>
                                        <h4 class="font-semibold text-gray-700 mb-1">
                                            <i class="fas fa-lightbulb text-yellow-600 mr-2"></i>
                                            Dica importante:
                                        </h4>
                                        <p class="text-gray-600 leading-relaxed">${exercise.tips}</p>
                                    </div>
                                </div>
                            </div>
                            <div class="flex-shrink-0 lg:w-80">
                                <div class="bg-gray-100 rounded-lg overflow-hidden">
                                    <img src="${exercise.image}" alt="${exercise.alt}" class="w-full h-48 object-cover">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            `;
            
            return card;
        }

        function showDashboard() {
            document.getElementById('workout-detail').classList.add('hidden');
            document.getElementById('dashboard').classList.remove('hidden');
            stopTimer();
        }

        function startTimer(seconds) {
            clearInterval(timerInterval);
            currentTime = seconds;
            updateTimerDisplay();
            
            const circle = document.getElementById('progress-circle');
            const circumference = 2 * Math.PI * 28;
            
            timerInterval = setInterval(() => {
                currentTime--;
                updateTimerDisplay();
                
                const progress = (seconds - currentTime) / seconds;
                const offset = circumference - (progress * circumference);
                circle.style.strokeDashoffset = offset;
                
                if (currentTime <= 0) {
                    clearInterval(timerInterval);
                    playNotification();
                }
            }, 1000);
        }

        function stopTimer() {
            clearInterval(timerInterval);
            currentTime = 90;
            updateTimerDisplay();
            const circle = document.getElementById('progress-circle');
            circle.style.strokeDashoffset = 251.2;
        }

        function updateTimerDisplay() {
            document.getElementById('timer-display').textContent = currentTime;
        }

        function playNotification() {
            // Simple notification for timer completion
            if ('Notification' in window && Notification.permission === 'granted') {
                new Notification('Descanso finalizado!', {
                    body: 'Hora de fazer a pr√≥xima s√©rie!',
                    icon: 'data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor"><path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm-2 15l-5-5 1.41-1.41L10 14.17l7.59-7.59L19 8l-9 9z"/></svg>'
                });
            } else {
                alert('Descanso finalizado! Hora da pr√≥xima s√©rie!');
            }
        }

        // Request notification permission on page load
        if ('Notification' in window && Notification.permission === 'default') {
            Notification.requestPermission();
        }

        // Initialize timer display
        updateTimerDisplay();

        // Service Worker for offline functionality
        if ('serviceWorker' in navigator) {
            window.addEventListener('load', () => {
                navigator.serviceWorker.register('data:application/javascript;base64,')
                .then(registration => {
                    console.log('FitApp registrado para uso offline!');
                })
                .catch(error => {
                    console.log('Erro no registro offline:', error);
                });
            });
        }

        // Detect offline/online status
        function updateOnlineStatus() {
            if (navigator.onLine) {
                document.body.classList.remove('offline');
            } else {
                document.body.classList.add('offline');
            }
        }

        window.addEventListener('online', updateOnlineStatus);
        window.addEventListener('offline', updateOnlineStatus);
        updateOnlineStatus();

        // Store workout progress locally
        function saveWorkoutProgress(day, exerciseIndex, sets, reps) {
            const progress = JSON.parse(localStorage.getItem('fitapp-progress') || '{}');
            const date = new Date().toISOString().split('T')[0];
            
            if (!progress[date]) progress[date] = {};
            if (!progress[date][day]) progress[date][day] = {};
            
            progress[date][day][exerciseIndex] = { sets, reps, timestamp: Date.now() };
            localStorage.setItem('fitapp-progress', JSON.stringify(progress));
        }

        function loadWorkoutProgress(day, exerciseIndex) {
            const progress = JSON.parse(localStorage.getItem('fitapp-progress') || '{}');
            const date = new Date().toISOString().split('T')[0];
            
            return progress[date]?.[day]?.[exerciseIndex] || null;
        }

        // Enhanced exercise card with progress tracking
        function createExerciseCardWithProgress(exercise, number, day) {
            const card = document.createElement('div');
            card.className = 'exercise-card rounded-xl p-6 shadow-lg';
            const progress = loadWorkoutProgress(day, number - 1);
            
            card.innerHTML = `
                <div class="flex items-start space-x-4">
                    <div class="flex-shrink-0">
                        <div class="w-12 h-12 bg-gradient-to-r from-purple-500 to-blue-500 rounded-full flex items-center justify-center text-white font-bold text-lg">
                            ${number}
                        </div>
                    </div>
                    <div class="flex-grow">
                        <div class="flex flex-col lg:flex-row lg:items-start lg:space-x-6">
                            <div class="flex-grow mb-4 lg:mb-0">
                                <h3 class="text-xl font-bold text-gray-800 mb-2">${exercise.name}</h3>
                                <div class="bg-gradient-to-r from-purple-100 to-blue-100 rounded-lg p-3 mb-4">
                                    <div class="flex items-center space-x-4">
                                        <div class="flex items-center space-x-2">
                                            <i class="fas fa-dumbbell text-purple-600"></i>
                                            <span class="font-semibold text-purple-800">${exercise.sets}</span>
                                        </div>
                                        <div class="flex items-center space-x-2">
                                            <i class="fas fa-clock text-blue-600"></i>
                                            <span class="text-blue-800">60-90s descanso</span>
                                        </div>
                                        ${progress ? `<div class="flex items-center space-x-2">
                                            <span class="text-green-600">‚úì Conclu√≠do hoje</span>
                                        </div>` : ''}
                                    </div>
                                </div>
                                <div class="space-y-3">
                                    <div>
                                        <h4 class="font-semibold text-gray-700 mb-1">
                                            <i class="fas fa-play-circle text-green-600 mr-2"></i>
                                            Como executar:
                                        </h4>
                                        <p class="text-gray-600 leading-relaxed">${exercise.instructions}</p>
                                    </div>
                                    <div>
                                        <h4 class="font-semibold text-gray-700 mb-1">
                                            <i class="fas fa-lightbulb text-yellow-600 mr-2"></i>
                                            Dica importante:
                                        </h4>
                                        <p class="text-gray-600 leading-relaxed">${exercise.tips}</p>
                                    </div>
                                    <div class="mt-4">
                                        <button onclick="markExerciseComplete('${day}', ${number - 1})" 
                                                class="bg-green-500 hover:bg-green-600 text-white px-4 py-2 rounded-lg transition-colors">
                                            ${progress ? 'J√° Conclu√≠do ‚úì' : 'Marcar como Conclu√≠do'}
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="flex-shrink-0 lg:w-80">
                                <div class="bg-gray-100 rounded-lg overflow-hidden">
                                    <img src="${exercise.image}" alt="${exercise.alt}" class="w-full h-48 object-cover">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            `;
            
            return card;
        }

        function markExerciseComplete(day, exerciseIndex) {
            saveWorkoutProgress(day, exerciseIndex, 'completed', 'completed');
            showWorkout(day); // Refresh to show updated status
        }

        // Update showWorkout function to use new card creation
        function showWorkoutWithProgress(day) {
            const workout = workouts[day];
            document.getElementById('dashboard').classList.add('hidden');
            document.getElementById('workout-detail').classList.remove('hidden');
            
            document.getElementById('workout-title').textContent = workout.title;
            document.getElementById('workout-subtitle').textContent = workout.subtitle;
            
            const container = document.getElementById('exercises-container');
            container.innerHTML = '';
            
            workout.exercises.forEach((exercise, index) => {
                const exerciseCard = createExerciseCardWithProgress(exercise, index + 1, day);
                container.appendChild(exerciseCard);
            });
        }

        // Override original showWorkout function
        showWorkout = showWorkoutWithProgress;
    </script>
</body>
</html>

```

